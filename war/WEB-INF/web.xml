<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Yakir_NASDAQ_Project</servlet-name>
		<servlet-class>com.mycompany.stock.servlet.Yakir_NASDAQ_ProjectServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Yakir_NASDAQ_Project</servlet-name>
		<url-pattern>/yakir_nasdaq_project</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>MathServlet</servlet-name>
		<servlet-class>com.mycompany.stock.servlet.MathServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MathServlet</servlet-name>
		<url-pattern>/mathservlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>AdvancedMathServlet</servlet-name>
		<servlet-class>com.mycompany.stock.servlet.AdvancedMathServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AdvancedMathServlet</servlet-name>
		<url-pattern>/advancedmathservlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>StockDetailsServlet</servlet-name>
		<servlet-class>com.mycompany.stock.servlet.StockDetailsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StockDetailsServlet</servlet-name>
		<url-pattern>/stockdetailsservlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>PortfolioServlet</servlet-name>
		<servlet-class>com.mycompany.stock.servlet.PortfolioServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PortfolioServlet</servlet-name>
		<url-pattern>/portfolioservlet</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
